{
  "name": "room-booking-system",
  "version": "0.4.3",
  "description": "Test task for the 'scrambly.io'",
  "author": "Denis Lutsky <denis.lutsky@outslook.com>",
  "private": true,
  "scripts": {
    "preinstall": "node preinstall.js",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "allow-migration": "node premigrate.js",
    "migration:create": "npm run allow-migration && npm run build && mikro-orm migration:create",
    "migration:up": "npm run allow-migration && npm run build && mikro-orm migration:up",
    "migration:down": "npm run allow-migration  && npm run build && mikro-orm migration:down",
    "database:fresh": "npm run allow-migration && npm run build && mikro-orm migration:fresh --seed"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.1.12",
    "@mikro-orm/core": "^6.1.12",
    "@mikro-orm/migrations": "^6.1.12",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/postgresql": "^6.1.12",
    "@mikro-orm/seeder": "^6.1.12",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.5",
    "@nestjs/core": "^10.3.5",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.3.5",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-yet": "^4.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "src/configs/mikro-orm.config.ts",
      "dist/configs/mikro-orm.config.js"
    ]
  },
  "engines": {
    "yarn": "Please use NPM instead",
    "pnpm": "Please use NPM instead",
    "npm": "10.x",
    "node": "20.x"
  }
}
